*background:            #272827
*foreground:            #657b83
*fading:                15
*fadeColor:             black
*cursorColor:           #16A085
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085

URxvt.internalBorder: 6

! special
*.foreground:   #c5c8c6
*.background:   #1e1e20
*.cursorColor:  #c5c8c6

! black
*.color0:       #1e1e20
*.color8:       #a1abad

! red
*.color1:       #80969b
*.color9:       #80969b

! green
*.color2:       #56737a
*.color10:      #56737a

! yellow
*.color3:       #416269
*.color11:      #416269

! blue
*.color4:       #2c5159
*.color12:      #2c5159

! magenta
*.color5:       #274850
*.color13:      #274850

! cyan
*.color6:       #1e383e
*.color14:      #1e383e

! white
*.color7:       #a1abad
*.color15:      #a1abad


! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #1e1e20, #274850, #274850
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #1e1e20, #c5c8c6, #1e1e20, #1e1e20, #274850
rofi.color-active: #1e1e20, #c5c8c6, #1e1e20, #1e1e20, #274850
rofi.color-urgent: #1e1e20, #c5c8c6, #1e1e20, #1e1e20, #274850

rofi.separator-style: solid
rofi.sidebar-mode: true
!rofi.font: Hack Regular 12
rofi.font: Fira Code Nerd Font 12
rofi.bw: 1
rofi.columns: 2
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
rofi.show-icons: true
rofi.lines: 12
